{
  "compilerOptions": {
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  }
}
